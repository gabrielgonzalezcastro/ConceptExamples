<Window x:Class="GeoLib.WindowsHost.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <StackPanel Orientation="Vertical">
        <Button Name="btnStart" Content="Start Services" Margin="50,20,50,0" FontSize="18" Click="BtnStart_OnClick"></Button>
        <Button Name="btnStop" Content="Stop Services" Margin="50,20,50,0" FontSize="18" Click="BtnStop_OnClick"></Button>
        <Label Name="lblMessage" Margin="50,20,50,0" HorizontalAlignment="Center" ></Label>
        <Button x:Name="btnInProc" Content="In-Proc Service" Margin="50,20,50,0" HorizontalAlignment="Left" FontSize="18" Click="BtnInProc_OnClick" />
    </StackPanel>
</Window>
